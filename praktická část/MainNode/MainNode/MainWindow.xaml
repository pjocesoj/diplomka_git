<Window x:Class="HlavniUzel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HlavniUzel"
        xmlns:controls="clr-namespace:HlavniUzel.Controls"
        xmlns:viewmodels="clr-namespace:HlavniUzel.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="add" HorizontalAlignment="Left" Margin="315,243,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button Content="load" HorizontalAlignment="Left" Margin="215,243,0,0" VerticalAlignment="Top" Command="{Binding LoadNodesCommand}"/>

    </Grid>
</Window>
